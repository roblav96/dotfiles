{
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"htmlWhitespaceSensitivity": "ignore",
	"jsxSingleQuote": false,
	"printWidth": 100,
	"proseWrap": "never",
	"quoteProps": "consistent",
	"semi": false,
	"singleQuote": true,
	"spaceRedirects": false,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,
	"variant": 0,
	"vueIndentScriptAndStyle": false,
	"xmlSelfClosingSpace": false,
	"xmlWhitespaceSensitivity": "ignore",
	"overrides": [
		{
			"files": [
				"*.hidden-color-scheme",
				"*.JSON-tmLanguage",
				"*.last-run",
				"*.sublime-build",
				"*.sublime-color-scheme",
				"*.sublime-commands",
				"*.sublime-completions",
				"*.sublime-keymap",
				"*.sublime-macro",
				"*.sublime-menu",
				"*.sublime-mousemap",
				"*.sublime-project",
				"*.sublime-settings",
				"*.sublime-theme",
				"*.sublime-workspace",
				"*.sublime_metrics",
				"*.sublime_session",
				".prettierrc",
			],
			"options": {
				"parser": "json5",
				"quoteProps": "preserve",
				"singleQuote": false,
			},
		},
		{
			"files": [
				"*.csproj",
				"*.plist",
				"*.storyboard",
				"*.sublime-snippet",
				"*.svg",
				"*.tmLanguage",
				"*.tmPreferences",
				"*.tmTheme",
				"*.xml",
			],
			"options": {
				"parser": "xml",
			},
		},
		{
			"files": [
				"*.sublime-syntax",
				"*.sublime-syntax.yaml-macros",
				"*.yaml",
				"*.YAML-tm*",
				"*.yml",
				"h2o.conf",
			],
			"options": {
				"parser": "yaml",
				"singleQuote": false,
				"tabWidth": 2,
				"useTabs": false,
			},
		},
		{
			"files": ["*.tml", "*.toml"],
			"options": {
				"parser": "toml",
				"singleQuote": false,
				"tabWidth": 2,
				"useTabs": false,
			},
		},
		{
			"files": ["*.java-properties", "*.java"],
			"options": {
				"parser": "java",
				"singleQuote": false,
				"tabWidth": 4,
				"useTabs": false,
			},
		},
		{
			"files": ["*.cs", "*.cake", "*.cshtml", "*.csx"],
			"options": {
				"parser": "cs",
				"singleQuote": false,
				"tabWidth": 4,
				"useTabs": false,
			},
		},
		{
			"files": ["*.kt", "*.ktm", "*.kts"],
			"options": {
				"parser": "kotlin",
				"singleQuote": false,
				"tabWidth": 4,
				"useTabs": false,
			},
		},
		{
			"files": ["*.css", "*.less", "*.sass", "*.scss"],
			"options": {
				"singleQuote": false,
			},
		},
		{
			"files": [
				"*.aw",
				"*.blade.php",
				"*.ctp",
				"*.fcgi",
				"*.inc",
				"*.php",
				"*.php3",
				"*.php4",
				"*.php5",
				"*.php_cs",
				"*.php_cs.dist",
				"*.phps",
				"*.phpt",
				"*Phakefile",
			],
			"options": {
				"parser": "html",
			},
		},
		{
			"files": ["*.d.ts"],
			"options": {
				"semi": true,
			},
		},
		{
			"files": [".ignore"],
			"options": {
				"parser": "sh",
			},
		},
	],
}
