{
	"addTrailingCommas": true,
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "ignore",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"printWidth": 100,
	"proseWrap": "never",
	"quoteProps": "consistent",
	"semi": false,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,
	"vueIndentScriptAndStyle": false,
	"xmlWhitespaceSensitivity": "ignore",
	"overrides": [
		{
			"files": [
				"*.avsc",
				"*.geojson",
				"*.gltf",
				"*.gson",
				"*.har",
				"*.hidden-color-scheme",
				"*.ice",
				"*.json",
				"*.JSON-tmLanguage",
				"*.jsonc",
				"*.jsonl",
				"*.map",
				"*.mcmeta",
				"*.sublime-build",
				"*.sublime-color-scheme",
				"*.sublime-commands",
				"*.sublime-completions",
				"*.sublime-keymap",
				"*.sublime-macro",
				"*.sublime-menu",
				"*.sublime-mousemap",
				"*.sublime-project",
				"*.sublime-settings",
				"*.sublime-theme",
				"*.sublime-workspace",
				"*.sublime_metrics",
				"*.sublime_session",
				"*.tfstate",
				"*.tfstate.backup",
				"*.topojson",
				"*.webapp",
				"*.webmanifest",
				"*.yy",
				"*.yyp",
				".arcconfig",
				".babelrc",
				".eslintrc",
				".eslintrc.json",
				".htmlhintrc",
				".jscsrc",
				".jshintrc",
				".jslintrc",
				".prettierrc",
				".tern-config",
				".tern-project",
				".watchmanconfig",
				"composer.lock",
				"language-configuration.json",
				"mcmod.info",
			],
			"options": {
				"parser": "json5",
				"quoteProps": "preserve",
				"singleQuote": false,
			},
		},
		{
			"files": ["jsconfig.json", "package.json", "tsconfig.json"],
			"options": {
				"parser": "json",
			},
		},
		{
			"files": [
				"*.csproj",
				"*.plist",
				"*.storyboard",
				"*.svg",
				"*.tmLanguage",
				"*.sublime-snippet",
				"*.tmPreferences",
				"*.tmTheme",
				"*.xml",
			],
			"options": {
				"parser": "xml",
			},
		},
		{
			"files": [
				"*.sublime-syntax",
				"*.sublime-syntax.yaml-macros",
				"*.yaml",
				"*.YAML-tm*",
				"*.yml",
				"h2o.conf",
			],
			"options": {
				"parser": "yaml",
				"singleQuote": false,
				"tabWidth": 2,
				"useTabs": false,
			},
		},
		{
			"files": ["*.tml", "*.toml"],
			"options": {
				"parser": "toml",
				"singleQuote": false,
				"tabWidth": 2,
				"useTabs": false,
			},
		},
		{
			"files": [
				"*.aw",
				"*.blade.php",
				"*.ctp",
				"*.fcgi",
				"*.inc",
				"*.php",
				"*.php3",
				"*.php4",
				"*.php5",
				"*.php_cs",
				"*.php_cs.dist",
				"*.phps",
				"*.phpt",
				"*Phakefile",
			],
			"options": {
				"parser": "html",
			},
		},
	],
}
